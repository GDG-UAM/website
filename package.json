{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "tsx scripts/sync-feature-flags.ts",
    "dev": "next dev --turbopack",
    "dev:classic": "next dev",
    "flags:sync": "tsx scripts/sync-feature-flags.ts",
    "next-build": "next build --webpack",
    "build": "npm run sitemap && cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "build-local": "npm run sitemap && cross-env NEXT_TELEMETRY_DISABLED=1 next build --webpack",
    "start": "cross-env NEXT_TELEMETRY_DISABLED=1 next start",
    "lint": "next lint",
    "sitemap": "tsx scripts/sitemap.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "{src,translations,scripts}/**/*.{js,jsx,ts,tsx,json,css,html}": [
      "prettier --write",
      "eslint --fix --max-warnings=0 --no-warn-ignored --rule 'import/no-unused-modules: off'"
    ]
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/eden": "^1.4.5",
    "@elysiajs/swagger": "^1.3.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@getbrevo/brevo": "^3.0.1",
    "@heroui/button": "^2.2.24",
    "@heroui/theme": "^2.4.20",
    "@mui/material": "^7.3.1",
    "@sentry/nextjs": "^10.32.1",
    "blurhash": "^2.0.5",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "fs": "^0.0.1-security",
    "highlight.js": "^11.11.1",
    "html-react-parser": "^5.1.16",
    "jszip": "^3.10.1",
    "marked": "^17.0.1",
    "marked-highlight": "^2.2.2",
    "mongoose": "^9.1.1",
    "next": "^16.1.1",
    "next-auth": "^4.24.11",
    "next-intl": "^4.3.4",
    "path": "^0.12.7",
    "postcss": "^8.4.49",
    "qrcode": "^1.5.4",
    "react": "^19.2.1",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "sanitize-html": "^2.13.0",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.19",
    "swr": "^2.3.6",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@types/jszip": "^3.4.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.2",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  }
}
